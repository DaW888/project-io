{
  "name": "nextjs-template-wajda",
  "author": "Dawid Wajda (wajda.io)",
  "version": "0.0.1",
  "description": "Next.js template with ts and tailwind",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier \"{pages,lib}/**/*.{ts,tsx,css}\"",
    "format": "yarn run prettier --write",
    "lint:fix": "yarn run lint --fix",
    "validate": "yarn run prettier --list-different && yarn run lint"
  },
  "dependencies": {
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.1.5",
    "@tailwindcss/forms": "^0.5.0",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  }
}
